{
  "name": "scorpion-security-hub-server",
  "version": "1.0.0",
  "description": "Node.js API and AI backend for Scorpion Security Hub with Supabase PostgreSQL support",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "server": "node src/index.js",
    "dev": "node --watch src/index.js",
    "migrate": "node src/db/migrate.js",
    "seed": "node src/db/seed-data.js",
    "create-admin": "node src/db/create-admin.js",
    "health": "node -e \"require('./src/db/database').healthCheck().then(console.log)\""
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "cybersecurity",
    "security-hub",
    "threat-intelligence",
    "incident-response",
    "supabase",
    "postgresql",
    "api"
  ],
  "author": "Scorpion Security Team",
  "license": "MIT"
}
